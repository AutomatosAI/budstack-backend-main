{
    "family": "backend-prod-env",
    "containerDefinitions": [
      {
        "name": "backend-prod-container",
        "image": "992382608811.dkr.ecr.us-east-1.amazonaws.com/backend_production:latest",
        "cpu": 2048,
        "memory": 4096,
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/backend-prod-env",
            "awslogs-region": "us-east-1",
            "awslogs-create-group": "true",
            "awslogs-stream-prefix": "ecs"
          }
        },
        
        "portMappings": [
          {
            "containerPort": 3000,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [
          {
            "name": "NODE_ENV",
            "value": "staging"
          }
        ],
        "secrets": [
          {
            "name": "PROD_PORT",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_PORT"
          },
          {
            "name": "PROD_DATABASE_URL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_DATABASE_URL"
          },
          {
            "name": "PROD_AWS_BUCKET_NAME",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_AWS_BUCKET_NAME"
          },
          {
            "name": "PROD_AWS_ACCESS_KEY_ID",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_AWS_ACCESS_KEY_ID"
          },
          {
            "name": "PROD_AWS_SECRET_ACCESS_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_AWS_SECRET_ACCESS_KEY"
          },
          {
            "name": "PROD_AWS_REGION",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_AWS_REGION"
          },
          {
            "name": "PROD_SENDGRID_API_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_SENDGRID_API_KEY"
          },
          {
            "name": "PROD_ADMIN_EMAIL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_ADMIN_EMAIL"
          },
          {
            "name": "PROD_FRONTEND_URL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_FRONTEND_URL"
          },
          {
            "name": "PROD_JWT_SECRET_LOGIN",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_JWT_SECRET_LOGIN"
          },
          {
            "name": "PROD_JWT_SECRET_VERIFY_EMAIL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_JWT_SECRET_VERIFY_EMAIL"
          },
          {
            "name": "PROD_JWT_SECRET_2FA",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_JWT_SECRET_2FA"
          },
          {
            "name": "PROD_NFT_CONTRACT_ID",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_NFT_CONTRACT_ID"
          },
          {
            "name": "PROD_NFT_MARKETPLACE_CONTRACT_NAME",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_NFT_MARKETPLACE_CONTRACT_NAME"
          },
          {
            "name": "PROD_NFT_MARKETPLACE_CONTRACT_ID",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_NFT_MARKETPLACE_CONTRACT_ID"
          },
          {
            "name": "PROD_CONTRACT_DEPLOYED_CHAIN_ID",
