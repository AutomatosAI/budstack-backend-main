          },
          {
            "name": "PROD_FIREBASE_CLIENT_EMAIL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_FIREBASE_CLIENT_EMAIL"
          },
          {
            "name": "PROD_FIREBASE_PROJECT_ID",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_FIREBASE_PROJECT_ID"
          },
          {
            "name": "PROD_FIREBASE_PRIVATE_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_FIREBASE_PRIVATE_KEY"
          },
          {
            "name": "PROD_FIREBASE_NOTIFICATION_IMAGE_URL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_FIREBASE_NOTIFICATION_IMAGE_URL"
          },
          {
            "name": "PROD_COIN_REMITTER_INVOICE_URL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_COIN_REMITTER_INVOICE_URL"
          },
          {
            "name": "PROD_COIN_REMITTER_TEST_API_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_COIN_REMITTER_TEST_API_KEY"
          },
          {
