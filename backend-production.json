          },
          {
            "name": "PROD_KYC_CLIENT_SECRET",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_KYC_CLIENT_SECRET"
          },
          {
            "name": "PROD_KYC_AUDIENCE",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_KYC_AUDIENCE"
          },
          {
            "name": "PROD_KYC_GRANT_TYPE",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_KYC_GRANT_TYPE"
          },
          {
