          },
          {
            "name": "PROD_COIN_REMITTER_ETH_API_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_COIN_REMITTER_ETH_API_KEY"
          },
          {
            "name": "PROD_COIN_REMITTER_ETH_PASSWORD",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_COIN_REMITTER_ETH_PASSWORD"
          },
          {
            "name": "PROD_COIN_REMITTER_BTC_API_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_COIN_REMITTER_BTC_API_KEY"
          },
          {
            "name": "PROD_COIN_REMITTER_BTC_PASSWORD",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_COIN_REMITTER_BTC_PASSWORD"
          },
          {
            "name": "PROD_PAYINN_API_KEY",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_PAYINN_API_KEY"
          },
          {
            "name": "PROD_PAYINN_API_URL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_PAYINN_API_URL"
          },
          {
            "name": "PROD_SUPPORT_ADMIN_EMAIL",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_SUPPORT_ADMIN_EMAIL"
          },  
          {
            "name": "PROD_NETWORK",
            "valueFrom": "arn:aws:ssm:us-east-1:992382608811:parameter/PROD_NETWORK"
          }  
        ]
      }
    ],
    "executionRoleArn": "arn:aws:iam::992382608811:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "runtimePlatform": {
      "operatingSystemFamily": "LINUX"
    },
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "2048",
    "memory": "4096"
  }
